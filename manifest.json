{
  "manifest_version": 3,
  "name": "Duh",
  "version": "0.0.0.1",
  "description": "A Chrome extension to get voucher codes for your favourite stores",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "host_permissions": ["https://duh-api.netlify.app/api"]
}